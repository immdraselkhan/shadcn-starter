  "scripts": {
    "dev": "next dev --turbopack",
    "build": "rimraf .next && next build > deploy.log 2>&1",
    "start": "fuser -k \"${PORT:-3000}\"/tcp && yarn next start >> deploy.log 2>&1 &",
    "update@latest": "yarn upgrade && npx syncyarnlock --save --keepPrefix && yarn install",
    "upgrade@latest": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
